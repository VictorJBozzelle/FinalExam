@model VehicleListViewModel

@if( Model.Vehicles.Count()==0 )
{
    <div class="text-center m-2">
        <form asp-action="SeedDatabase" method="post">
            <button type="submit" class="btn btn-danger">Seed Database</button>
        </form>
    </div>
}
else
{
    @foreach( var v in Model.Vehicles )
    {
        @Html.Partial("VehicleList", v)
    }
}
<div class="text-center">
    <a asp-action="Create" class="btn btn-primary">Add Vehicle</a>
</div>
<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" class="btn-group pull-right m-1">
</div>
